{
  "build": {
    "lfs": true,
    "nixpacks": {
      "config": {
        "setup": [
          "apt-get update",
          "apt-get install -y libgl1 git git-lfs",
          "git lfs install",
          "git lfs fetch origin main",
          "git lfs checkout",
          "find Model_Train -type f -exec file {} \\;"
        ],
        "variables": {
          "TF_ENABLE_ONEDNN_OPTS": "0",
          "GIT_LFS_SKIP_SMUDGE": "0"
        }
      }
    }
  }
}
