{
  "build": {
    "lfs": true,
    "nixpacks": {
      "config": {
        "setup": [
          "apt-get update",
          "apt-get install -y libgl1 git git-lfs",
          "git lfs install",
          "git lfs pull",
          "find Model_Train -type f -exec ls -lh {} \\;"
        ],
        "variables": {
          "TF_ENABLE_ONEDNN_OPTS": "0"
        }
      }
    }
  }
}
